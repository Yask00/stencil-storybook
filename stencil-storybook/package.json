{
  "name": "stencil-storybook",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-storybook/stencil-storybook.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "pack": "npm pack",
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build-stencil:watch": "stencil build --docs-readme --watch --serve",
    "start-storybook": "start-storybook -p 6006 -s dist"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@stencil/angular-output-target": "0.0.5",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/html": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "cssnano": "^4.1.10",
    "postcss-replace": "^1.1.3",
    "tailwindcss": "^2.0.4"
  }
}
